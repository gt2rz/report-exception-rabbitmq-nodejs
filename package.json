{
  "name": "nodejs-docker-template",
  "version": "1.0.0",
  "description": "nodejs-docker-template",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc -p . --outDir ./dist",
    "dev": "nodemon",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write src/**/*.ts",
    "test": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.9.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/amqplib": "^0.10.3",
    "@types/dotenv": "8.2.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "3.0.1",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint-fix",
      "git add"
    ]
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "dotenv": "^16.3.1"
  }
}
